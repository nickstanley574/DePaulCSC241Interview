{
"saveAndRun": {
    "commands": [
      {
        "match": ".*",
        "cmd": "./generate.py",
        "useShortcut": false,
        "silent": false
      },
      {
        "match": "\\.txt$",
        "cmd": "echo 'I am a .txt file ${file}.'",
        "useShortcut": false,
        "silent": false
      }
    ]
  }
}